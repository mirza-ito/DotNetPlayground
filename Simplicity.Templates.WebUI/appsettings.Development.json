{
  "ApplicationOptions": {
    "Setup": {
      "EnableHttpLogging": true,
      "EnableMiniProfiler": true
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Information",
        "System": "Information"
      }
    }
  }
}